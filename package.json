  "bugs": {
    "url": "https://github.com/TREEcg/LDES-Action/issues"
  },
  "homepage": "https://github.com/TREEcg/LDES-Action#readme",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@treecg/actor-init-ldes-client": "2.3.8",
    "date-fns": "^2.23.0",
    "n3": "^1.11.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.29.2",
    "env-cmd": "^10.1.0",
